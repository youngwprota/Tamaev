            let upbtn = td2d.appendChild(document.createElement('span'));
            upbtn.innerText = 'â–²'
            clickify([upbtn, downbtn, delbtn, editbtn]);

            delbtn.onclick = function() {
                saveUpdate({"sq_del_child": i}, false, saveState);
