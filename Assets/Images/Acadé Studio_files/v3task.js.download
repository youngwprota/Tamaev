                changeBtn.innerText = 'Update';
                let cancelBtn = pane.appendChild(document.createElement('button'));
                cancelBtn.style.marginLeft = '10px'
                cancelBtn.innerText = 'Cancel';

                changeBtn.onclick = function() {
